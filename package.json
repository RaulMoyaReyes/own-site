{
  "name": "own-site",
  "version": "0.6.0",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "whatwg-fetch": "^2.0.3",
    "promise": "^8.0.1",
    "object-assign": "^4.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "1.1.3",
    "css-loader": "^0.28.10",
    "dotenv": "^5.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.10",
    "firebase-tools": "^3.17.3",
    "fs-extra": "5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.2",
    "jest-junit": "^3.6.0",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "^2.1.1",
    "raf": "^3.4.0",
    "react-dev-utils": "^5.0.0",
    "style-loader": "^0.20.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "^0.6.2",
    "webpack": "^4.0.1",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --ci --testResultsProcessor=jest-junit --coverage",
    "test:watch": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "coverageDirectory": "results/coverage",
    "coverageReporters": [
      "lcov",
      "text-summary",
      "clover"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx,mjs}",
      "src/containers/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/enzyme.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/?(*.)(spec|test).{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "coveragePathIgnorePatterns": [
      "/__test?(s)__/"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "results/tests/jest-junit.xml"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
