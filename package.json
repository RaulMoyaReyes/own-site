{
  "name": "own-site",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "simple-line-icons": "^2.4.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "dotenv-expand": "^4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.3.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "firebase-tools": "^4.0.0",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "jest-junit": "^5.1.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^2.1.6",
    "raf": "^3.4.0",
    "react-dev-utils": "^5.0.1",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.4",
    "webpack-dev-server": "^2.11.2",
    "webpack-manifest-plugin": "^2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:dev": "npm run build -- --env=local",
    "test": "node scripts/test.js --env=jsdom --ci --testResultsProcessor=jest-junit --coverage",
    "test:watch": "node scripts/test.js --env=jsdom --watch --coverage"
  },
  "jest": {
    "coverageDirectory": "results/coverage",
    "coverageReporters": [
      "lcov",
      "text-summary",
      "clover"
    ],
    "collectCoverageFrom": [
      "src/*/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/enzyme.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/?(*.)(spec|test).{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "coveragePathIgnorePatterns": [
      "/__test?(s)__/",
      "/services/",
      "/utils/",
      "/data/"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "results/tests/jest-junit.xml"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
